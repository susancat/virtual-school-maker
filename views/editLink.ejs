<%- include("./partials/header") %>
<%- include("./partials/vsmHeader") %>

<div class="container">
    <div class="row">
        <h1 class="text-center create_div" style="width:40%; margin: auto;">Edit Game Links</h1>
    </div>
    <div class="row">
        <div class="create_div" style="width:40%; margin: 25px auto;">
            <form action="/allCourses?_method=PUT" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text" name="course[gamelink1]" placeholder="Game 1">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="course[gamelink2]" placeholder="Game 2">
                </div>
                <div class="form-inline">
                    <a href="/dashboard" class="btn btn-lg btn-secondary" style="width:49%">Cancel</a>
                    <button class="btn btn-lg btn-primary ml-1" style="width:49%;">Submit</button>
                </div>
            </form>
        </div>    
    </div>   
</div>
<%- include("./partials/footer") %>